{
  "version": 2,
  "builds": [{ "src": "./src/index.js", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/src/index.js" }],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://pixelize-eta.vercel.app/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
